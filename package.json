{
	"name": "city-furshet-backoffice",
	"productName": "City Furshet Backoffice",
	"description": "Desktop app back-office for city-furshet.ru",
	"repository": "https://github.com/deniska69/city-furshet-backoffice.git",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"main": "dist-electron/main.js",
	"scripts": {
		"dev": "npm-run-all --parallel dev:r dev:e",
		"dev:r": "vite",
		"build:r": "tsc -b && vite build",
		"build:r:ts": "tsc -b && vite build && rm -rf dist-react",
		"preview:r": "vite preview",
		"dev:e": "npm run transpile:e && cross-env NODE_ENV=development electron .",
		"transpile:e": "tsc --project src/electron/tsconfig.json",
		"build:e:win": "npm run transpile:e && npm run build:r && electron-builder --win --x64"
	},
	"author": {
		"name": "deniska69",
		"email": "gostev.dy@yandex.ru"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@tailwindcss/vite": "^4.0.9",
		"basic-ftp": "^5.0.5",
		"class-variance-authority": "^0.7.1",
		"classnames": "^2.5.1",
		"dotenv": "^16.4.7",
		"iconv-lite": "^0.6.3",
		"mobx": "^6.13.6",
		"mobx-react": "^9.2.0",
		"papaparse": "^5.5.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-hook-form": "^7.54.2",
		"react-hot-toast": "^2.5.2",
		"react-router": "^7.2.0",
		"short-unique-id": "^5.2.0",
		"tailwindcss": "^4.0.9"
	},
	"devDependencies": {
		"@eslint/js": "^9.21.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@types/papaparse": "^5.3.15",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"cross-env": "^7.0.3",
		"electron": "^34.3.0",
		"electron-builder": "^25.1.8",
		"eslint": "^9.21.0",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^15.15.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.24.1",
		"vite": "^6.2.0"
	}
}
